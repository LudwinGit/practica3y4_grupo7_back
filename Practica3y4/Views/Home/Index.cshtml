@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <div class="row">
        <div class="col-md-6">
            <img src="https://cdn.tn.com.ar/sites/default/files/styles/1366x765/public/2017/01/11/caja_ahorro_chicos1.jpg"  class="img-responsive"/>
        </div>
        <div class="col-md-6">
            <form role="form">
                <div class="form-group">
                    <label for="usuario">Usuario</label>
                    <input type="text" class="form-control" name="usuario" autofocus />
                </div>

                <div class="form-group">
                    <label for="password">Contraseña</label>
                    <input type="password" class="form-control" name="password" />
                </div>

                <input type="submit" class="btn btn-primary" value="Ingresar" />
            </form>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <h2>Getting started</h2>
        <table>
            <thead>
            </thead>
            <tbody>
                @foreach (var dia in @ViewBag.fechas)
                {
                <tr>
                    <td>@dia.fecha</td>
                    <td>@dia.venta</td>
                    <td>@dia.compra</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>